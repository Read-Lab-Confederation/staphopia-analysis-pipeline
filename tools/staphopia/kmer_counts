#! /usr/bin/env python
"""
kmer_counts.

Bit encode kmers.
"""

if __name__ == '__main__':
    import sys
    from bitarray import bitarray

    d = {
        'G': bitarray('00'),
        'A': bitarray('01'),
        'T': bitarray('10'),
        'C': bitarray('11')
    }
    for line in sys.stdin.readlines():
        line = line.rstrip()
        seq, count = line.split()
        a = bitarray()
        a.encode(d, seq)
        print '{0}\t{1}'.format(a.tobytes(), count)
